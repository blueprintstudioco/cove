{
  "version": 2,
  "buildCommand": "echo 'no build'",
  "outputDirectory": "landing",
  "routes": [
    { "src": "/v1/(.*)", "dest": "/api/index.js" },
    { "src": "/health", "dest": "/api/index.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3"
    }
  }
}
